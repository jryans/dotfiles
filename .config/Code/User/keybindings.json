// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "ctrl+k right",
    "command": "workbench.action.moveEditorToNextGroup",
    "when": "!isMac"
  },
  {
    "key": "cmd+k right",
    "command": "workbench.action.moveEditorToNextGroup",
    "when": "isMac"
  },

  {
    "key": "ctrl+k left",
    "command": "workbench.action.moveEditorToPreviousGroup",
    "when": "!isMac"
  },
  {
    "key": "cmd+k left",
    "command": "workbench.action.moveEditorToPreviousGroup",
    "when": "isMac"
  },

  {
    "key": "cmd+1",
    "command": "workbench.action.openEditorAtIndex1",
    "when": "isMac"
  },
  {
    "key": "cmd+2",
    "command": "workbench.action.openEditorAtIndex2",
    "when": "isMac"
  },
  {
    "key": "cmd+3",
    "command": "workbench.action.openEditorAtIndex3",
    "when": "isMac"
  },
  {
    "key": "cmd+4",
    "command": "workbench.action.openEditorAtIndex4",
    "when": "isMac"
  },
  {
    "key": "cmd+5",
    "command": "workbench.action.openEditorAtIndex5",
    "when": "isMac"
  },
  {
    "key": "cmd+6",
    "command": "workbench.action.openEditorAtIndex6",
    "when": "isMac"
  },
  {
    "key": "cmd+7",
    "command": "workbench.action.openEditorAtIndex7",
    "when": "isMac"
  },
  {
    "key": "cmd+8",
    "command": "workbench.action.openEditorAtIndex8",
    "when": "isMac"
  },
  {
    "key": "cmd+9",
    "command": "workbench.action.openEditorAtIndex9",
    "when": "isMac"
  },

  {
    "key": "ctrl+enter",
    "command": "editor.action.replaceOne",
    "when": "!isMac && editorFocus && findWidgetVisible"
  },
  {
    "key": "cmd+enter",
    "command": "editor.action.replaceOne",
    "when": "isMac && editorFocus && findWidgetVisible"
  },

  {
    "key": "cmd+shift+1",
    "command": "-editor.action.replaceOne",
    "when": "editorFocus && findWidgetVisible"
  },
  {
    "key": "ctrl+shift+1",
    "command": "-editor.action.replaceOne",
    "when": "editorFocus && findWidgetVisible"
  },

  {
    "key": "ctrl+enter",
    "command": "search.action.replace",
    "when": "!isMac && isEditableItem && matchFocus && replaceActive && searchViewletVisible"
  },
  {
    "key": "cmd+enter",
    "command": "search.action.replace",
    "when": "isMac && isEditableItem && matchFocus && replaceActive && searchViewletVisible"
  },
  {
    "key": "cmd+shift+1",
    "command": "-search.action.replace",
    "when": "isEditableItem && matchFocus && replaceActive && searchViewletVisible"
  },
  {
    "key": "ctrl+shift+1",
    "command": "-search.action.replace",
    "when": "isEditableItem && matchFocus && replaceActive && searchViewletVisible"
  },
  {
    "key": "ctrl+shift+1",
    "command": "-search.action.replaceAllInFile",
    "when": "fileMatchFocus && isEditableItem && replaceActive && searchViewletVisible"
  },
  {
    "key": "ctrl+shift+1",
    "command": "-search.action.replaceAllInFolder",
    "when": "folderMatchFocus && isEditableItem && replaceActive && searchViewletVisible"
  },

  {
    "key": "ctrl+numpad_add",
    "command": "editor.action.fontZoomIn",
    "when": "!isMac"
  },
  {
    "key": "cmd+numpad_add",
    "command": "editor.action.fontZoomIn",
    "when": "isMac"
  },
  {
    "key": "ctrl+shift+=",
    "command": "editor.action.fontZoomIn",
    "when": "!isMac"
  },
  {
    "key": "cmd+shift+=",
    "command": "editor.action.fontZoomIn",
    "when": "isMac"
  },
  {
    "key": "ctrl+=",
    "command": "editor.action.fontZoomIn",
    "when": "!isMac"
  },
  {
    "key": "cmd+=",
    "command": "editor.action.fontZoomIn",
    "when": "isMac"
  },

  {
    "key": "ctrl+numpad_subtract",
    "command": "editor.action.fontZoomOut",
    "when": "!isMac"
  },
  {
    "key": "cmd+numpad_subtract",
    "command": "editor.action.fontZoomOut",
    "when": "isMac"
  },
  {
    "key": "ctrl+shift+-",
    "command": "editor.action.fontZoomOut",
    "when": "!isMac"
  },
  {
    "key": "cmd+shift+-",
    "command": "editor.action.fontZoomOut",
    "when": "isMac"
  },
  {
    "key": "ctrl+-",
    "command": "editor.action.fontZoomOut",
    "when": "!isMac"
  },
  {
    "key": "cmd+-",
    "command": "editor.action.fontZoomOut",
    "when": "isMac"
  },

  {
    "key": "ctrl+numpad0",
    "command": "editor.action.fontZoomReset",
    "when": "!isMac"
  },
  {
    "key": "cmd+numpad0",
    "command": "editor.action.fontZoomReset",
    "when": "isMac"
  },
  {
    "key": "ctrl+0",
    "command": "editor.action.fontZoomReset",
    "when": "!isMac"
  },
  {
    "key": "cmd+0",
    "command": "editor.action.fontZoomReset",
    "when": "isMac"
  },

  {
    "key": "ctrl+q",
    "command": "-workbench.action.quit"
  },
  {
    "key": "cmd+q",
    "command": "-workbench.action.quit"
  },

  {
    "key": "ctrl+shift+w",
    "command": "-workbench.action.closeWindow"
  },
  {
    "key": "cmd+shift+w",
    "command": "-workbench.action.closeWindow"
  },

  {
    "key": "cmd+m",
    "command": "-markdown.extension.editing.toggleMath",
    "when": "editorTextFocus && !editorReadonly && editorLangId == 'markdown'"
  },
  {
    "key": "cmd+m cmd+shift+b",
    "command": "-latex-workshop.shortcut.mathbb",
    "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$/"
  },
  {
    "key": "cmd+m cmd+b",
    "command": "-latex-workshop.shortcut.mathbf",
    "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$/"
  },
  {
    "key": "cmd+m cmd+c",
    "command": "-latex-workshop.shortcut.mathcal",
    "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$/"
  },
  {
    "key": "cmd+m cmd+i",
    "command": "-latex-workshop.shortcut.mathit",
    "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$/"
  },
  {
    "key": "cmd+m cmd+r",
    "command": "-latex-workshop.shortcut.mathrm",
    "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$/"
  },
  {
    "key": "cmd+m cmd+s",
    "command": "-latex-workshop.shortcut.mathsf",
    "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$/"
  },
  {
    "key": "cmd+m cmd+t",
    "command": "-latex-workshop.shortcut.mathtt",
    "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$/"
  },
  {
    "key": "cmd+alt+c",
    "command": "-latex-workshop.clean",
    "when": "!config.latex-workshop.bind.altKeymap.enabled && !virtualWorkspace && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$|^pweave$/"
  },
  {
    "key": "cmd+m",
    "command": "-markdown.extension.editing.toggleMath",
    "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^markdown$|^rmd$|^quarto$/"
  },

  {
    "key": "ctrl+shift+g f",
    "command": "gitlens.openFileOnRemote",
    "when": "!isMac"
  },
  {
    "key": "cmd+alt+g f",
    "command": "gitlens.openFileOnRemote",
    "when": "isMac"
  },

  {
    "key": "ctrl+shift+g m",
    "command": "gitlens.openCommitOnRemote",
    "when": "!isMac"
  },
  {
    "key": "cmd+alt+g m",
    "command": "gitlens.openCommitOnRemote",
    "when": "isMac"
  },

  {
    "key": "ctrl+shift+g y",
    "command": "gitlens.copyRemoteFileUrlToClipboard",
    "when": "!isMac"
  },
  {
    "key": "cmd+alt+g y",
    "command": "gitlens.copyRemoteFileUrlToClipboard",
    "when": "isMac"
  },

  {
    "key": "alt+left",
    "command": "workbench.action.navigateBack",
    "when": "canNavigateBack"
  },
  {
    "key": "alt+right",
    "command": "workbench.action.navigateForward",
    "when": "canNavigateForward"
  },

  {
    "key": "ctrl+alt+s",
    "command": "workbench.action.files.saveFiles",
    "when": "!isMac"
  },

  {
    "key": "ctrl+shift+i",
    "command": "-workbench.action.toggleDevTools",
    "when": "isDevelopment"
  },

  {
    "key": "ctrl+g",
    "command": "editor.action.nextMatchFindAction",
    "when": "!isMac && editorFocus"
  },
  {
    "key": "f3",
    "command": "-editor.action.nextMatchFindAction",
    "when": "!isMac && editorFocus"
  },
  {
    "key": "shift+f3",
    "command": "-editor.action.previousMatchFindAction",
    "when": "!isMac && editorFocus"
  },
]
